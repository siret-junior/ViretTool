<Application x:Class="ViretTool.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ViretTool">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary>
					<local:AppBootstrapper x:Key="AppBootstrapper" />
				</ResourceDictionary>
				<!-- Other dictionaries can be placed here -->
                <ResourceDictionary Source="pack://application:,,,/ViretTool.PresentationLayer;component/Styles/BusyIndicatorStyle.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
